{
  "name": "geo-map",
  "version": "1.0.0",
  "description": "A facade that abstracts over the Google and HERE map JavaScript SDKs",
  "files": [
    "index.js",
    "index.d.ts",
    "lib",
    "esm"
  ],
  "main": "index.js",
  "module": "esm/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "npm run build:commonjs && npm run build:esm",
    "build:commonjs": "tsc --project .",
    "build:esm": "tsc --project . --outdir esm --module esnext",
    "test": "jest -c jest.unit.js",
    "test:integration": "jest -c jest.integration.js",
    "lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "marionebl/geo-map"
  },
  "license": "AFL-2.0",
  "bugs": {
    "url": "https://github.com/marionebl/geo-map/issues"
  },
  "homepage": "https://github.com/marionebl/geo-map#readme",
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/expect-puppeteer": "^3.3.0",
    "@types/express": "^4.16.0",
    "@types/googlemaps": "^3.30.11",
    "@types/heremaps": "^3.0.12",
    "@types/jest": "^23.3.0",
    "@types/jest-environment-puppeteer": "^2.2.1",
    "@types/jsdom": "^11.0.6",
    "@types/node": "^10.5.2",
    "@types/puppeteer": "^1.6.4",
    "@types/query-string": "^6.1.0",
    "@types/uuid": "^3.4.3",
    "execa": "^1.0.0",
    "express": "^4.16.3",
    "jest": "^23.4.1",
    "jest-canvas-mock": "^1.1.0",
    "jest-cli": "^23.4.1",
    "jest-puppeteer": "^3.4.0",
    "jsdom": "^11.11.0",
    "jsdom-simulant": "^1.1.2",
    "png.js": "^0.2.1",
    "prettier": "^1.13.7",
    "puppeteer": "^1.6.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.10.3",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "mobx": "4.3.1",
    "query-string": "^6.1.0",
